    <!--
    Interface
    -->
    <wsdl:interface name="VoDKATVInterface">

        <!-- Room -->
        <wsdl:operation name="CreateRoom"
            pattern="http://www.w3.org/ns/wsdl/in-out"
            style="http://www.w3.org/ns/wsdl/style/iri"
            wsdlx:safe="true">

            <wsdl:documentation>
            It creates a new household with the specified values.
            </wsdl:documentation>

            <wsdl:input element="msg:createRoomParams"/>
            <wsdl:output element="msg:room"/>
        </wsdl:operation>

        <wsdl:operation name="FindAllRooms"
            pattern="http://www.w3.org/ns/wsdl/in-out"
            style="http://www.w3.org/ns/wsdl/style/iri"
            wsdlx:safe="true">

            <wsdl:documentation>
            It returns all households already created in the system.
            </wsdl:documentation>

            <wsdl:output element="msg:rooms"/>
        </wsdl:operation>

        <wsdl:operation name="DeleteRoom"
            pattern="http://www.w3.org/ns/wsdl/in-out"
            style="http://www.w3.org/ns/wsdl/style/iri"
            wsdlx:safe="true">

            <wsdl:documentation>
            It deletes an existing household and all their associated devices
            from the system.
            </wsdl:documentation>

            <wsdl:input element="msg:deleteRoomParams"/>
            <wsdl:output element="msg:rooms"/>
        </wsdl:operation>

        <!-- Device -->
        <wsdl:operation name="CreateDevice"
            pattern="http://www.w3.org/ns/wsdl/in-out"
            style="http://www.w3.org/ns/wsdl/style/iri"
            wsdlx:safe="true">

            <wsdl:documentation>
            It creates a new device with the specified values.
            </wsdl:documentation>

            <wsdl:input element="msg:createDeviceParams"/>
            <wsdl:output element="msg:device"/>
        </wsdl:operation>

        <wsdl:operation name="FindDeviceById"
            pattern="http://www.w3.org/ns/wsdl/in-out"
            style="http://www.w3.org/ns/wsdl/style/iri"
            wsdlx:safe="true">

            <wsdl:documentation>
            It returns the device with the given identifier.
            </wsdl:documentation>

            <wsdl:input element="msg:findDeviceByIdParams"/>
            <wsdl:output element="msg:device"/>
        </wsdl:operation>

        <wsdl:operation name="FindDevices"
            pattern="http://www.w3.org/ns/wsdl/in-out"
            style="http://www.w3.org/ns/wsdl/style/iri"
            wsdlx:safe="true">

            <wsdl:documentation>
            It returns the list of devices filtering by a given criteria.
            </wsdl:documentation>

            <wsdl:input element="msg:findDevicesParams"/>
            <wsdl:output element="msg:devices"/>
        </wsdl:operation>

        <wsdl:operation name="FindDevicesByRoom"
            pattern="http://www.w3.org/ns/wsdl/in-out"
            style="http://www.w3.org/ns/wsdl/style/iri"
            wsdlx:safe="true">

            <wsdl:documentation>
            It returns a list of the devices from the specified household.
            </wsdl:documentation>

            <wsdl:input element="msg:findDevicesByRoomParams"/>
            <wsdl:output element="msg:devices"/>
        </wsdl:operation>

        <wsdl:operation name="FindDevicesByTagId"
            pattern="http://www.w3.org/ns/wsdl/in-out"
            style="http://www.w3.org/ns/wsdl/style/iri"
            wsdlx:safe="true">

            <wsdl:documentation>
            It returns the list of the devices with the specified tag node.
            </wsdl:documentation>

            <wsdl:input element="msg:findDevicesByTagIdParams"/>
            <wsdl:output element="msg:devices"/>
        </wsdl:operation>

        <wsdl:operation name="UpdateDevice"
            pattern="http://www.w3.org/ns/wsdl/in-out"
            style="http://www.w3.org/ns/wsdl/style/iri"
            wsdlx:safe="true">

            <wsdl:documentation>
            It updates the information associated to an already existing device.
            </wsdl:documentation>

            <wsdl:input element="msg:updateDeviceParams"/>
            <wsdl:output element="msg:device"/>
        </wsdl:operation>

        <wsdl:operation name="DeleteDevice"
            pattern="http://www.w3.org/ns/wsdl/in-out"
            style="http://www.w3.org/ns/wsdl/style/iri"
            wsdlx:safe="true">

            <wsdl:documentation>
            It deletes an existing device from the system.
            </wsdl:documentation>

            <wsdl:input element="msg:deleteDeviceParams"/>
            <wsdl:output element="msg:devices"/>
        </wsdl:operation>

        <!-- DeviceClasses -->
        <wsdl:operation name="FindAllDeviceClasses"
            pattern="http://www.w3.org/ns/wsdl/in-out"
            style="http://www.w3.org/ns/wsdl/style/iri"
            wsdlx:safe="true">

            <wsdl:documentation>
            It returns all the device classes available in the system.
            </wsdl:documentation>

            <wsdl:output element="msg:findAllDeviceClassesResponse"/>
        </wsdl:operation>

        <!-- Session -->
        <wsdl:operation name="CreateSession"
            pattern="http://www.w3.org/ns/wsdl/in-out"
            style="http://www.w3.org/ns/wsdl/style/iri"
            wsdlx:safe="true">

            <wsdl:documentation>
            It creates a new session in an existing household. This operation
            will also create a default account for the created session.
            </wsdl:documentation>

            <wsdl:input element="msg:createSessionParams"/>
            <wsdl:output element="msg:createSessionResponse"/>
        </wsdl:operation>

        <wsdl:operation name="FindSessionById"
            pattern="http://www.w3.org/ns/wsdl/in-out"
            style="http://www.w3.org/ns/wsdl/style/iri"
            wsdlx:safe="true">

            <wsdl:documentation>
            It returns the session with the given identifier.
            </wsdl:documentation>

            <wsdl:input element="msg:findSessionByIdParams"/>
            <wsdl:output element="msg:findSessionByIdResponse"/>
        </wsdl:operation>

        <wsdl:operation name="FindSessionsByRoom"
            pattern="http://www.w3.org/ns/wsdl/in-out"
            style="http://www.w3.org/ns/wsdl/style/iri"
            wsdlx:safe="true">

            <wsdl:documentation>
            It returns a list of the sessions created in the specified household.
            </wsdl:documentation>

            <wsdl:input element="msg:findSessionsByRoomParams"/>
            <wsdl:output element="msg:findSessionsByRoomResponse"/>
        </wsdl:operation>

        <wsdl:operation name="UpdateSession"
            pattern="http://www.w3.org/ns/wsdl/in-out"
            style="http://www.w3.org/ns/wsdl/style/iri"
            wsdlx:safe="true">

            <wsdl:documentation>
            It updates the information associated to an already existing session.
            </wsdl:documentation>

            <wsdl:input element="msg:updateSessionParams"/>
            <wsdl:output element="msg:updateSessionResponse"/>
        </wsdl:operation>

        <wsdl:operation name="DeleteSession"
            pattern="http://www.w3.org/ns/wsdl/in-out"
            style="http://www.w3.org/ns/wsdl/style/iri"
            wsdlx:safe="true">

            <wsdl:documentation>
            It deletes an existing session and all associated accounts from the system.
            </wsdl:documentation>

            <wsdl:input element="msg:deleteSessionParams"/>
            <wsdl:output element="msg:deleteSessionResponse"/>
        </wsdl:operation>

        <!-- Account -->
        <wsdl:operation name="CreateAccount"
            pattern="http://www.w3.org/ns/wsdl/in-out"
            style="http://www.w3.org/ns/wsdl/style/iri"
            wsdlx:safe="true">

            <wsdl:documentation>
            It creates a new account for a session. This operation will not
            associate any login/password to access the system. On the other
            hand, the value for outputsProtection} and parentalControlLevel}
            will be the default values defined on the VoDKATV system.
            </wsdl:documentation>

            <wsdl:input element="msg:createAccountParams"/>
            <wsdl:output element="msg:createAccountResponse"/>
        </wsdl:operation>

        <wsdl:operation name="FindAccountsBySessionAccounting"
            pattern="http://www.w3.org/ns/wsdl/in-out"
            style="http://www.w3.org/ns/wsdl/style/iri"
            wsdlx:safe="true">

            <wsdl:documentation>
            It returns the accounts defined for the specified session.
            </wsdl:documentation>

            <wsdl:input element="msg:findAccountsBySessionAccountingParams"/>
            <wsdl:output element="msg:findAccountsBySessionAccountingResponse"/>
        </wsdl:operation>

        <wsdl:operation name="UpdateAccount"
            pattern="http://www.w3.org/ns/wsdl/in-out"
            style="http://www.w3.org/ns/wsdl/style/iri"
            wsdlx:safe="true">

            <wsdl:documentation>
            It updates the information associated to an already existing account.
            </wsdl:documentation>

            <wsdl:input element="msg:updateAccountParams"/>
            <wsdl:output element="msg:updateAccountResponse"/>
        </wsdl:operation>

        <wsdl:operation name="DeleteAccount"
            pattern="http://www.w3.org/ns/wsdl/in-out"
            style="http://www.w3.org/ns/wsdl/style/iri"
            wsdlx:safe="true">

            <wsdl:documentation>
            It updates the information associated to an already existing account.
            </wsdl:documentation>

            <wsdl:input element="msg:deleteAccountParams"/>
            <wsdl:output element="msg:deleteAccountResponse"/>
        </wsdl:operation>

        <!-- AccountDetails -->
        <wsdl:operation name="CreateAccountDetails"
            pattern="http://www.w3.org/ns/wsdl/in-out"
            style="http://www.w3.org/ns/wsdl/style/iri"
            wsdlx:safe="true">

            <wsdl:documentation>
            It creates a new account for a session.
            </wsdl:documentation>

            <wsdl:input element="msg:createAccountDetailsParams"/>
            <wsdl:output element="msg:createAccountDetailsResponse"/>
        </wsdl:operation>

        <wsdl:operation name="FindAccountDetailsById"
