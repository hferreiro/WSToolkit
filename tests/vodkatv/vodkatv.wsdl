            <wsdl:input element="msg:findAccountDetailsByIdParams"/>
            <wsdl:output element="msg:findAccountDetailsByIdResponse"/>
        </wsdl:operation>

        <wsdl:operation name="FindAccountsDetailsBySessionAccounting"
